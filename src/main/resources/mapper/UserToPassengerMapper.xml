<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxc.ticketsys.dao.UserToPassengerDao">
    <select id="selectPassengerByUserId" parameterType="java.lang.Long" resultType="com.zxc.ticketsys.model.Passenger">
        select * from user_passenger where userId=#{userId};
    </select>
</mapper>